{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "JsonAutoServiceOptions": {
    "ConnectionString": "your db connection string",
    "Mode": "Debug",
    "ErrorThreshold": 50000,
    "DefaultErrorMessages": {
      "DELETE": "No records were deleted",
      "POST": "No records were recorded",
      "PUT": "No records were updated",
      "GET": "No records were found",
      "HEAD": "No response was returned",
      "Default": "Method not supported"
    },
    "RequiredHeaders": {
      "Client-Id": "ClientId"
    },
    "IdentityClaims": {}
  },
  "AllowedHosts": "*"
}
